  "editor.minimap.enabled": false,
  "eslint.nodePath": "./server/api:./app",
  "jest.autoEnable": false,
  "jest.pathToConfig": "jest.config.js",
  "cSpell.language": "en-GB",
  "itunes.titleStringLimit": 32,
  "workbench.colorTheme": "Nord",
  "workbench.preferredDarkColorTheme": "Nord",
  "workbench.preferredLightColorTheme": "Nord",
  "explorer.confirmDragAndDrop": false,
  "editor.columnSelection": false,
  "javascript.updateImportsOnFileMove.enabled": "never",
  "breadcrumbs.enabled": true,
  "sass.disableUnitCompletion": false,
  "flow.useBundledFlow": false,
  "editor.accessibilitySupport": "off",
  "eslint.onIgnoredFiles": "warn",
  "eslint.run": "onSave",
  "eslint.autoFixOnSave": true,
  "eslint.codeAction.showDocumentation": {
    "enable": true
  },
  "[json]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "extensions.ignoreRecommendations": false,
  "gitlens.remotes": [{
    "domain": "github.skyscannertools.net",
    "type": "Custom",
    "name": "My Company",
    "protocol": "https",
    "urls": {
        "repository": "https://github.skyscannertools.net/${repoBase}/${repoPath}",
        "branches": "https://github.skyscannertools.net/${repoBase}/${repoPath}/branches",
        "branch": "https://github.skyscannertools.net/${repoBase}/${repoPath}/commits/${branch}",
        "commit": "https://github.skyscannertools.net/${repoBase}/${repoPath}/commit/${id}",
        "file": "https://github.skyscannertools.net/${repoBase}/${repoPath}?path=${file}${line}",
        "fileInBranch": "https://github.skyscannertools.net/${repoBase}/${repoPath}/blob/${branch}/${file}${line}",
        "fileInCommit": "https://github.skyscannertools.net/${repoBase}/${repoPath}/blob/${id}/${file}${line}",
        "fileLine": "#L${line}",
        "fileRange": "#L${start}-L${end}"
        }
    }]
}