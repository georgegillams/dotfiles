{
  "window.autoDetectColorScheme": true,
  "eslint.lintTask.enable": true,
  "window.zoomLevel": 1,
  "editor.fontFamily": "Fira Mono For Powerline",
  "terminal.integrated.fontFamily": "Fira Mono For Powerline",
  "editor.fontLigatures": null,
  "vim.easymotion": true,
  "vim.incsearch": true,
  "vim.useSystemClipboard": true,
  "vim.useCtrlKeys": true,
  "vim.hlsearch": true,
  "vim.insertModeKeyBindings": [
    {
      "before": ["j", "j"],
      "after": ["<Esc>"]
    }
  ],
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": ["<leader>", "a"],
      "commands": ["workbench.action.findInFiles"]
    },
    {
      "before": ["?"],
      "commands": ["actions.find"]
    },
    {
      "before": ["<leader>", "c"],
      "commands": ["actions.find"]
    },
    {
      "before": ["<leader>", "v"],
      "commands": [":vsplit"]
    },
    {
      "before": ["<leader>", "s"],
      "commands": [":split"]
    },
    {
      "before": ["H"],
      "after": ["h", "h", "h", "h", "h", "h"]
    },
    {
      "before": ["J"],
      "after": ["j", "j", "j", "j", "j", "j"]
    },
    {
      "before": ["K"],
      "after": ["k", "k", "k", "k", "k", "k"]
    },
    {
      "before": ["L"],
      "after": ["l", "l", "l", "l", "l", "l"]
    }
  ],
  "vim.leader": "<space>",
  "prettier.jsxSingleQuote": true,
  "eslint.format.enable": true,
  "scssLint.runOnTextChange": true,
  "scssLint.showHighlights": true,
  "editor.formatOnSave": true,
  "window.nativeTabs": true,
  "firefox.executable": "/Applications/Firefox\\ Developer\\ Edition.app",
  "workbench.iconTheme": "vscode-great-icons",
  "eslint.alwaysShowStatus": true,
  "prettier.useEditorConfig": false,
  "sass.lint.enable": true,
  "stylelint.reportNeedlessDisables": true,
  "diffEditor.renderSideBySide": false,
  "editor.renderControlCharacters": false,
  "cSpell.userWords": [
    "CGWEDDING",
    "Categorised",
